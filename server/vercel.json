{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "index.js": {
      "maxDuration": 10,
      "memory": 1024,
      "maxRequestSize": "50mb"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
